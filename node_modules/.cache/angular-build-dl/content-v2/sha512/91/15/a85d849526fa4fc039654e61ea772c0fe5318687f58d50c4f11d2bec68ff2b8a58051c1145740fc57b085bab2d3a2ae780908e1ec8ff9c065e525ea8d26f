class AuthService {
    constructor(router, http) {
        this.router = router;
        this.http = http;
        this.isAuth$ = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](false);
    }
    createNewUser(email, password) {
        return new Promise((resolve, reject) => {
            this.http.post('http://localhost:3000/api/auth/signup', { email: email, password: password })
                .subscribe(() => {
                this.login(email, password).then(() => {
                    resolve();
                }).catch((error) => {
                    reject(error);
                });
            }, (error) => {
                reject(error);
            });
        });
    }
    login(email, password) {
        return new Promise((resolve, reject) => {
            this.http.post('http://localhost:3000/api/auth/login', { email: email, password: password })
                .subscribe((authData) => {
                this.token = authData.token;
                this.userId = authData.userId;
                this.isAuth$.next(true);
                resolve();
            }, (error) => {
                reject(error);
            });
        });
    }
    logout() {
        this.isAuth$.next(false);
        this.userId = null;
        this.token = null;
    }
}
