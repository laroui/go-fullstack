    _angular_core__WEBPACK_IMPORTED_MODULE_4__["ɵɵadvance"](2);
    _angular_core__WEBPACK_IMPORTED_MODULE_4__["ɵɵproperty"]("disabled", ctx_r1.part >= 3 && ctx_r1.userId !== (ctx_r1.thing == null ? null : ctx_r1.thing.userId));
