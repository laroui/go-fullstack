class AuthGuard {
    constructor(auth, state, router) {
        this.auth = auth;
        this.state = state;
        this.router = router;
    }
    canActivate(route, state) {
        return rxjs__WEBPACK_IMPORTED_MODULE_3__["Observable"].create((observer) => {
            this.auth.isAuth$.subscribe((auth) => {
                if (!auth) {
                    this.state.part$.subscribe((part) => {
                        if (part === 3) {
                            this.router.navigate(['/part-three/auth/login']);
                        }
                        else if (part === 4) {
                            this.router.navigate(['/part-four/auth/login']);
                        }
                    });
                }
                observer.next(true);
            });
        });
    }
}
